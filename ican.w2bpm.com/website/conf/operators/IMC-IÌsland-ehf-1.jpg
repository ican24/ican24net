			$("#triggermenu").addClass("collapsed");
			$("#menu_holder").css({'height':0});			
		}
		
		else
		{
			var heights = [];
			$("#menu_holder ul").each(function(i) {
				if($(this).is(":visible"))		
				{
					var holder_height = $(this).children("li:last").css("top").replace("px", "");		
		            var holder_height_clean =  parseFloat(holder_height) + parseFloat(46);
					
					heights.push( holder_height_clean ); 
					
				}
            });
			
			var set_holder_current_height =heights.pop();
			 
			$("#triggermenu").removeClass("collapsed");
			$("#menu_holder").css({'height':set_holder_current_height});
		}
				
	}); 


   $("body,html").on("click", function(evt) {
		$("#triggermenu").addClass("collapsed");
		$("#menu_holder").css({'height':0});
	}); 
	 
	 
	$("#menu,#triggermenu").on("click", function(evt) {	
	    evt.stopPropagation();
	})
	
	
	
