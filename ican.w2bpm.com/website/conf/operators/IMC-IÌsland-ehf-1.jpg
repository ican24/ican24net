function set_menu_height(){
	 
	if(viewport().width > 480 && viewport().width < 769  )
	{
		
		
		$('.box').each(function(){
			if($(this).find('.half:not(".changed")').length < 2 && $(this).find('.half:not(".changed")').length > 0)
			{
				$(this).css('paddingTop','24.25%');
			    $(this).find('.half').addClass('changed').css('height','100%');				
			}
		
		});
		
		
		
		var side_mneu = $('.right .box:first');
		
		var side_mneu_height = parseInt(side_mneu.find('.side_menu').height());
		
		
		var reference = $('.right .box:eq(1)');
		reference.append("<div class='reference' style='position:absolute; left:0; right:0; top:0; width:100%; height:100%; display:none;'><div><div class='margin' style='position:absolute; left:0; right:0; top:0; width:100%; height:2%; display:none;'><div>");
		var reference_height = parseInt($('.reference').height());				
		var half = reference_height/2;
		
		var margin = parseInt($('.margin').height());	 
		
		if(side_mneu_height > reference_height)
		{
			side_mneu.css('minHeight',reference_height+half+margin);
			$('.right .box:eq(2)').css('marginLeft','1%');
			
			if(side_mneu_height > reference_height+half)
			{
				side_mneu.css('minHeight',reference_height*2+margin*2);
				$('.right .box:eq(2)').css('marginLeft','1%');
			}
		}
		else
		{
			side_mneu.css('minHeight',reference_height);
			$('.right .box:eq(2)').css({'marginLeft':'1%','float':'right','marginRight':0});
