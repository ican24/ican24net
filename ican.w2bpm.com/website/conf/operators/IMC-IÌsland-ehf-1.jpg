function filter_mobile_menu(){
	
	$("#menu_holder").find("li").each(function(){		
		  
		  $(this).removeAttr("class");		    
		   
		  var has_child = $(this).find("ul").length;
		  
		  if(has_child > 0)
		  {						  
			  $(this).addClass("has_child");				 		    
			  $(this).children("ul").prepend("<li class='back'><a href='javascript:void(0)'>"+$(this).children("a").text()+"</a></li><li class='just_link'><a href='"+$(this).children("a").prop('href')+"'>Overview</a></li>"); 
			   $(this).children("a").prop("href","javascript:void(0)");	
		      //$(this).children("a").remove();			  
		  }
		  else
		  {
			 $(this).addClass("just_link");		 
		  }		  
		  
	  });
	      
	 
	$("#menu_holder").find("li").each(function(){
				  
		   var set_top = ($(this).prevUntil($(this).closest("ul")).length) * 46;
		  
		   $(this).css({"top":set_top+"px"}); 		   
		   
		   $(this).find("ul").not("#menu").css({"marginTop":-set_top+"px"});	
	          
	});
	  	
	
	
	var do_anything = false;	 
	
	 $(".has_child").each(function(){
	
	 	
	 $(this).on("click", function() {     
		
		  if (do_anything==false)
		   {
		  
		  do_anything=true;
		  	   
		  var show_this = $(this).children("ul");
	      show_this.show();
		  
		  var holder_height = show_this.children("li:last").css("top").replace("px", "");		  
		  
		  var holder_height_clean =  parseFloat(holder_height) + parseFloat(46)+"px";
		  
		  $("#menu_holder").height("1000px");
		  	  
	       $(this).parent("ul").stop("true","false")
            .animate(
                { 
				    left:"-=100%" }, {
                    duration: 300,
                    easing: 'easeOutQuad',
					complete: function(){
					$("#menu_holder").height(holder_height_clean);
					do_anything = false;
					
					}
            });
			
		   }
		
	
	     });
	 }); 
 

 
 	$("#menu_holder .back").on("click", function() {
		     
			if (do_anything==false)
		    {
			 do_anything=true;	
		 	 var hide_this = $(this).parent("ul");	
			 $("#menu_holder").height("1000px");			
			 $(this).parent().parent().parent("ul").stop("true","false")
            .animate(
                { 
				    left:"+=100%" }, {
                    duration: 300,
                    easing: 'easeOutQuad',
					complete: function(){
						hide_this.hide();  
						 
						var holder_height = $(this).children("li:last").css("top").replace("px", "");
		
		                var holder_height_clean =  parseFloat(holder_height) + parseFloat(46)+"px";
						
						$("#menu_holder").height(holder_height_clean);
						
						do_anything = false;
						
						}
                });
				
			}	
	 
	 }); 
	 
 	 
	 
	 $("li").click(function(e) {
        e.stopPropagation();
     });
	 
	 
	 
} // end filter_mobile_menu
	

  $("#triggermenu").on("click", function() {
	  	    
		var has_height = $("#menu_holder").height(); 		
		
		if(has_height!=0)
